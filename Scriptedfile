node {  
   stages {
        stage('Clone repository') {
            steps {
                git 'https://github.com/varunmh28/devopspracs.git'
            }
        }
        stage('Deploy to XAMPP') {
            steps {
                sh "scp -r -o StrictHostKeyChecking=no *.html localhost@HTTP/1.1:/opt/xampp/htdocs/"
            }
        }
    }
}
